{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/client-cli",
  "version": "1.0.0",
  "description": "CLI operations as RPC procedures - lib.refresh, lib.scan, DAG traversal",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "schematics": "./schematics/collection.json",
  "files": [
    "dist",
    "src",
    "schematics"
  ],
  "scripts": {
    "build": "tsc -b && tsc -p schematics/tsconfig.json",
    "build:schematics": "tsc -p schematics/tsconfig.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo schematics/**/*.js schematics/**/*.d.ts schematics/**/*.map",
    "postinstall": "client announce"
  },
  "client": {
    "procedures": "./dist/register.js"
  },
  "dependencies": {
    "@mark1russell7/client": "github:mark1russell7/client#main",
    "@mark1russell7/client-logger": "github:mark1russell7/client-logger#main",
    "@angular-devkit/schematics": "^19.0.0",
    "ts-morph": "^24.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^22.0.0",
    "@mark1russell7/cue": "github:mark1russell7/cue#main"
  },
  "keywords": [
    "cli",
    "client",
    "procedures",
    "rpc",
    "dag",
    "npm"
  ],
  "license": "MIT",
  "sideEffects": false,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@mark1russell7/cue",
      "@mark1russell7/client",
      "@mark1russell7/client-logger"
    ]
  }
}
